{
	"name": "pokemon-index",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"test:jest": "jest  --coverage",
		"test:watch": "jest --watchAll --verbose --coverage",
		"cypress": "cypress open",
		"test:cypress": "cypress run",
		"test:mocha-reporter": "cypress run --reporter mochawesome --reporter-options 'reportDir=cypress/report/mochawesome-report,overwrite=false,html=false,json=true,timestamp=mmddyyyy_HHMMss'",
		"test:mocha-report": "mochawesome-merge cypress/report/mochawesome-report/mochawesome*.json > cypress/report/mochawesome.json",
		"test:static": "npm run lint && npm run tsc",
		"test:e2e": "(amplify mock api &) && wait-on http-get://localhost:20002 && kill-port 3000 && (npm run dev &) && wait-on http-get://localhost:3000 && cypress run --env PORT=3000",
		"test:e2e:dev": "(amplify mock api &) && wait-on http-get://localhost:20002 && kill-port 3000 && (npm run dev &) && wait-on http-get://localhost:3000 && cypress open --env PORT=3000",
		"test": "npm run test:static && npm run test:jest"
	},
	"hooks": {
		"pre-commit": "prettier --write \"src/*.ts\" \"src/**/*.ts*\"",
		"pre-push": "npm run test"
	},
	"dependencies": {
		"@types/node": "9.6.7",
		"@types/react": "^17.0.29",
		"@aws-amplify/api": "^1.0.42",
		"@aws-amplify/pubsub": "^1.1.0",
		"axios": "^0.22.0",
		"cypress": "^8.6.0",
		"eslint-plugin-cypress": "^2.12.1",
		"next": "^11.1.0",
		"next-pwa": "^5.3.1",
		"next-themes": "0.0.15",
		"tailwindcss": "^1.7.3",
		"prop-types": "^15.7.2",
		"react": "17.0.2",
		"react-dom": "^17.0.2",
		"react-error-boundary": "^3.1.3",
		"react-icons": "^4.3.1",
		"wait-on": "^6.0.0"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.2",
		"@types/react-dom": "^17.0.9",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^27.2.5",
		"wait-on": "^3.3.0",
		"eslint": "^7.32.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-next": "11.1.2",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.25.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^27.2.5",
		"mocha": "^5.2.0",
		"mochawesome": "^6.3.1",
		"mochawesome-merge": "^4.2.0",
		"mochawesome-report-generator": "^5.2.0",
		"pm2": "^5.1.2",
		"postcss-preset-env": "^6.7.0",
		"prettier": "^2.4.0",
		"react-test-renderer": "^17.0.2",
		"typescript": "4.4.3",
		"webpack": "^5.58.2",
		"workbox-webpack-plugin": "^6.3.0"
	}
}
